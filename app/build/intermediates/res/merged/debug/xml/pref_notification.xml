<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- A 'parent' preference, which enables/disables child preferences (below)
         when checked/unchecked. -->
    <ListPreference
        android:defaultValue="-1"
        android:entries="@array/pref_example_list_titles"
        android:entryValues="@array/pref_example_list_values"
        android:key="example_list"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_changeAlbum"/>

    <SwitchPreference
        android:defaultValue="true"
        android:key="@string/setting4"
        android:title="@string/setting4" />
    <SwitchPreference
        android:defaultValue="true"
        android:key="locationSetting"
        android:title="@string/setting3" />
    <SwitchPreference
        android:defaultValue="true"
        android:key="@string/setting1"
        android:title="@string/setting1" />
    <SwitchPreference
        android:defaultValue="true"
        android:key="@string/setting2"
        android:title="@string/setting2" />

</PreferenceScreen>
